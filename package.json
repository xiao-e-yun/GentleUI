{
  "name": "gentle-ui",
  "version": "1.0.0",
  "description": "GentleUI is a user interface design library that emphasizes rounded corners and a poetic aesthetic.",
  "main": "dist/main.umd.js",
  "module": "dist/main.js",
  "types": "dist/main.d.ts",
  "type": "module",
  "scripts": {
    "build": "vite build && vue-tsc",
    "watch": "vite build --watch"
  },
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.js"
    },
    "./base.scss": "./dist/assets/base.scss",
    "./_vars.scss": "./dist/assets/_vars.scss",
    "./_color.scss": "./dist/assets/_color.scss",
    "./base.module.scss": "./dist/assets/base.module.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiao-e-yun/GentleUI.git"
  },
  "keywords": [
    "UI",
    "Vue",
    "Scss"
  ],
  "author": "xiao-e-yun",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/xiao-e-yun/GentleUI/issues"
  },
  "homepage": "https://github.com/xiao-e-yun/GentleUI#readme",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist/**/*"
  ],
  "dependencies": {
    "typescript": "^4.9.4",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "pug": "^3.0.2",
    "sass": "^1.57.1",
    "vite": "^4.0.4",
    "vite-plugin-static-copy": "^0.13.0",
    "vue-tsc": "^1.0.24"
  }
}
